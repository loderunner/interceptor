{
  "name": "interceptor",
  "version": "1.0.0",
  "description": "A Chrome devtools extension for intercepting and modifying HTTP requests",
  "main": "out/index.js",
  "scripts": {
    "lint": "prettier --check . && eslint --max-warnings=0 .",
    "build": "esbuild src/background/index.ts src/panel/index.tsx --loader:.woff2=file --loader:.woff=file --bundle --minify --outdir=out",
    "watch": "esbuild src/background/index.ts src/panel/index.tsx --loader:.woff2=file --loader:.woff=file --bundle --sourcemap=inline --outdir=out --watch"
  },
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/chrome": "0.0.177",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "esbuild": "0.14.17",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-react": "7.28.0",
    "prettier": "2.5.1",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@fontsource/roboto": "^4.5.2",
    "@mui/icons-material": "^5.3.1",
    "@mui/material": "^5.3.1",
    "devtools-protocol": "^0.0.966116",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
